<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN" "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
<hibernate-mapping>  
<class name="com.javatpoint.mypack.Fees" table="feesupload">
<id name="tid">  
 <generator class="increment"></generator>  
 </id>
<property name="id" column="id"></property>
 <property name="fees" column="fees"></property>
 </class>  
</hibernate-mapping>
